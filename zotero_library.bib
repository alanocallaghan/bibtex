@article{Mar2011,
  title = {Variance of {{Gene Expression Identifies Altered Network Constraints}} in {{Neurological Disease}}},
  author = {Mar, Jessica C. and Matigian, Nicholas A. and {Mackay-Sim}, Alan and Mellick, George D. and Sue, Carolyn M. and Silburn, Peter A. and McGrath, John J. and Quackenbush, John and Wells, Christine A.},
  editor = {Gibson, Greg},
  year = {2011},
