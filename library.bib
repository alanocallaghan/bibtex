issn = {1932-6203},
journal = {PloS one},
month = {jan},
number = {12},
pages = {e114594},
pmid = {25490046},
title = {{CRISPR-Cas9-Based Knockout of the Prion Protein and Its Effect on the Proteome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25490046},
volume = {9},
year = {2014}
}
