@article{Cha2012,
abstract = {Exposure to ultraviolet B (UV-B) radiation has been associated with a variety of adverse effects in all forms of life, including micro-organisms, plants, animals and humans. Ultraviolet B induces cell damage at the molecular level and consequently organisms must employ strategies to protect themselves from sunlight and to repair UV-B-induced cellular damage. In this study, the UV-B protective effects of four different phlorotannins isolated from a brown alga (Ecklonia cava) were determined using zebrafish (Danio rerio) as an in vivo model. Zebrafish embryos were pretreated with phlorotannins and exposed to UV-B (50 mJ/cm(2)). The heart rate, generation of reactive oxygen species and nitric oxide, cell death and hyperpigmentation were assessed in order to evaluate UV-B-induced photo-damage. Treatment of the embryos with the algal phorotannins reduced UV-B-induced reactive oxygen species and nitric oxide levels, protected against UV-B-induced cell death and significantly reduced hyperpigmentation. We therefore suggest that phlorotannins isolated from E. cava can protect against UV-B radiation. Editor Note. Readers of the journal may be unfamiliar with the use of zebrafish embryos in research studies. There is no indication in this article of an ethical review of the study. This is because the use of fish embryos in research, at least in the UK, is not subject to a licensing procedure if they are less than 5 days post fertilization (dpf). In this study the embryos were 2 dpf.},
author = {Cha, Seon-Heui and Ko, Chang-Ik and Kim, Daekyung and Jeon, You-Jin},
doi = {10.1111/j.1365-3164.2011.01009.x},
file = {:home/alan/Documents/Papers/Cha et al. - 2012 - Protective effects of phlorotannins against ultraviolet B radiation in zebrafish (Danio rerio). - Veterinary dermato.pdf:pdf},
issn = {1365-3164},
journal = {Veterinary dermatology},
keywords = {Animals,Antioxidants,Antioxidants: pharmacology,Cell Death,Cell Death: drug effects,Embryo, Nonmammalian,Embryo, Nonmammalian: drug effects,Embryo, Nonmammalian: radiation effects,Hyperpigmentation,Hyperpigmentation: prevention {\&} control,Nitric Oxide,Nitric Oxide: metabolism,Phaeophyta,Phaeophyta: chemistry,Radiation Injuries, Experimental,Radiation Injuries, Experimental: prevention {\&} con,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Tannins,Tannins: pharmacology,Ultraviolet Rays,Ultraviolet Rays: adverse effects,Zebrafish},
month = {feb},
