title = {{Effect of phlorotannins isolated from Ecklonia cava on angiotensin I-converting enzyme (ACE) inhibitory activity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3085808{\&}tool=pmcentrez{\&}rendertype=abstract},
