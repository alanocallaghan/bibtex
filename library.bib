file = {:home/alan/Documents/Papers/Li, Dunson - 2016 - Simple , Scalable and Accurate Posterior Interval Estimation.pdf:pdf},
issn = {14643510},
journal = {Biometrika},
keywords = {Credible interval,Markov chain Monte Carlo,Quantile estimation,Wasserstein barycentre},
number = {3},
pages = {665--680},
title = {{Simple, scalable and accurate posterior interval estimation}},
url = {https://arxiv.org/pdf/1605.04029v1.pdf},
volume = {104},
year = {2017}
}
