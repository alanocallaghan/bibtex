isbn = {0046-8177},
issn = {00468177},
journal = {Human Pathology},
keywords = {Decision making,Digital pathology,Expertise,Radiology,Telepathology,Visual search,Whole slide images},
number = {3},
pages = {357--364},
pmid = {22835956},
publisher = {Elsevier Inc.},
title = {{Characterizing the development of visual search expertise in pathology residents viewing whole slide images}},
