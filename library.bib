keywords = {Aging/genetics,Animals,Antigen,CD4-Positive T-Lymphocytes/immunology,Cellular Senescence/genetics,Genetic Variation,Immunologic Memory/genetics,Inbred C57BL,Lymphocyte Activation/genetics,Male,Mice,RNA,Receptors,Sequence Analysis,Single-Cell Analysis,T-Cell/metabolism,Transcriptome},
