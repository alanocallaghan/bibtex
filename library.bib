1467-5463 (Linking)},
issn = {1467-5463},
journal = {Briefings in Bioinformatics},
keywords = {biomedical informatics,deep learning,electronic health,genomics,health care,translational bioinformatics},
number = {February},
pages = {1--11},
pmid = {28481991},
title = {{Deep learning for healthcare: review, opportunities and challenges}},
url = {https://academic.oup.com/bib/article-lookup/doi/10.1093/bib/bbx044},
