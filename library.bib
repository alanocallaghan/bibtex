journal = {Cytotechnology},
keywords = {activated protein kinase {\'{a}},mitogen-,nerve growth factor {\'{a}},pc12 cells {\'{a}} neurodifferentiation,signal pathway,vitamin b 12 {\'{a}},{\'{a}}},
month = {nov},
number = {3},
pages = {181--7},
pmid = {19002876},
title = {{Vitamin B(12), a chlorophyll-related analog to pheophytin a from marine brown algae, promotes neurite outgrowth and stimulates differentiation in PC12 cells.}},
