@article{Borji2012,
abstract = {Despite significant recent progress, the best available visual $\backslash$nsaliency models still lag behind human performance in predicting eye fixations $\backslash$nin free-viewing of natural scenes. Majority of models are based on low-level $\backslash$nvisual features and the importance of top-down factors has not yet been fully $\backslash$nexplored or modeled. Here, we combine low-level features such as orientation, $\backslash$ncolor, intensity, saliency maps of previous best bottom-up models with top-down $\backslash$ncognitive visual features (e.g., faces, humans, cars, etc.) and learn a direct $\backslash$nmapping from those features to eye fixations using Regression, SVM, and AdaBoost $\backslash$nclassifiers. By extensive experimenting over three benchmark eye-tracking $\backslash$ndatasets using three popular evaluation scores, we show that our boosting model $\backslash$noutperforms 27 state-of-the-art models and is so far the closest model to the $\backslash$naccuracy of human model for fixation prediction. Furthermore, our model $\backslash$nsuccessfully detects the most salient object in a scene without sophisticated $\backslash$nimage processings such as region segmentation.},
author = {Borji, Ali},
doi = {10.1109/CVPR.2012.6247706},
file = {:home/alan/Documents/Papers/Borji - 2012 - Boosting bottom-up and top-down visual features for saliency estimation - Proceedings of the IEEE Computer Society Confer.pdf:pdf},
isbn = {9781467312264},
issn = {10636919},
journal = {Proceedings of the IEEE Computer Society Conference on Computer Vision and Pattern Recognition},
pages = {438--445},
title = {{Boosting bottom-up and top-down visual features for saliency estimation}},
year = {2012}
}
