issn = {20452322},
journal = {Scientific Reports},
number = {December 2015},
pages = {1--16},
pmid = {27189340},
publisher = {Nature Publishing Group},
title = {{Whole-transcriptome analysis of UUO mouse model of renal fibrosis reveals new molecular players in kidney diseases}},
