journal = {Vision Research},
keywords = {Computational model,Eye movements,Natural images,Salience,Visual attention},
number = {1},
pages = {107--123},
pmid = {11804636},
title = {{Modeling the role of salience in the allocation of overt visual attention}},
volume = {42},
year = {2002}
}
