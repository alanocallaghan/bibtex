keywords = {Alanine Transaminase,Alanine Transaminase: blood,Animals,Aspartate Aminotransferases,Aspartate Aminotransferases: blood,Benzofurans,Benzofurans: pharmacology,Body Weight,Body Weight: drug effects,Cholesterol,Cholesterol: blood,Glutathione Peroxidase,Glutathione Peroxidase: metabolism,Hepatitis, Alcoholic,Hepatitis, Alcoholic: pathology,Hepatitis, Alcoholic: prevention {\&} control,Lipid Peroxidation,Lipid Peroxidation: drug effects,Liver,Liver: pathology,Male,Malondialdehyde,Malondialdehyde: blood,Mice,Mice, Inbred BALB C,Republic of Korea,Seaweed,Seaweed: chemistry,Survival,Tannins,Tannins: pharmacology,Thiobarbituric Acid Reactive Substances,Thiobarbituric Acid Reactive Substances: metabolis},
