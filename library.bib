issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Brain,Brain: metabolism,Female,Gene Expression Profiling,Gene Expression Profiling: methods,Genome, Human,Humans,Linear Models,Male},
month = {jun},
number = {11},
pages = {1555--61},
pmid = {24519379},
title = {{Analysis of gene expression data using a linear mixed model/finite mixture model approach: application to regional differences in the human brain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24519379},
volume = {30},
year = {2014}
}
