file = {:home/alan/Documents/Papers/Srivastava, Li, Dunson - 2015 - Scalable Bayes via Barycenter in Wasserstein Space.pdf:pdf},
isbn = {0070600706657},
issn = {0026-9247},
keywords = {barycenter,big data,distributed bayesian computations,empirical measures,linear,optimal transportation,programming,wasserstein distance,wasserstein space},
title = {{Scalable Bayes via Barycenter in Wasserstein Space}},
url = {http://arxiv.org/abs/1508.05880},
