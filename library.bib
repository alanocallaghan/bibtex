keywords = {about such data integration,and,autonomy,bioinformatics,biological data integration,concerns,databases,from our point of,heterogeneity,there are three main,view,web services and federated},
number = {May 2017},
pages = {334--338},
title = {{A Comparison of Federated Databases with Web Services for the Integration of Bioinformatics Data}},
