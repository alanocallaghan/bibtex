issn = {2325-4416},
journal = {Medical science monitor basic research},
month = {jan},
pages = {138--42},
pmid = {25149683},
title = {{Comparing bioinformatic gene expression profiling methods: microarray and RNA-Seq.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4152252{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {20},
year = {2014}
}
