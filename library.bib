journal = {The Annals of Statistics},
keywords = {Bernoulli factory,Monte Carlo methods,Poisson estimator,Sign problem,Unbiased estimator},
number = {2},
pages = {769--784},
title = {{On nonnegative unbiased estimators}},
volume = {43},
year = {2015}
}
