issn = {22111247},
journal = {Cell Reports},
keywords = {X-chromosome,embryo,epiblast,gastrulation,primitive endoderm,primitive streak,single-cell RNA-seq,transcriptional noise},
number = {5},
pages = {1215--1228},
pmid = {28768204},
title = {{Single-Cell Landscape of Transcriptional Heterogeneity and Cell Fate Decisions during Mouse Early Gastrulation}},
url = {http://www.sciencedirect.com/science/article/pii/S2211124717309610},
volume = {20},
year = {2017}
