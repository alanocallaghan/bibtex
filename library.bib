pmid = {22766839},
title = {{Tumor suppressive microRNA‑138 contributes to cell migration and invasion through its targeting of vimentin in renal cell carcinoma.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3582944{\&}tool=pmcentrez{\&}rendertype=abstract},
