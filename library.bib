issn = {1476-4687 (Electronic); 0028-0836 (Linking)},
journal = {Nature},
number = {7570},
pages = {479--485},
pmid = {26375005},
title = {{Epicardial FSTL1 reconstitution regenerates the adult mammalian heart.}},
volume = {525}
}
