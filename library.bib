journal = {Journal of Pathology Informatics},
keywords = {access this article online,clinical routine,digital pathology,digital pathology workflow,remote,whole slide imaging,work},
pages = {14},
pmid = {24843825},
title = {{Implementation of large-scale routine diagnostics using whole slide imaging in Sweden: Digital pathology experiences 2006-2013}},
url = {http://www.jpathinformatics.org/text.asp?2014/5/1/14/129452},
volume = {5},
year = {2014}
